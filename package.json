{
  "name": "ifeomajonah",
  "version": "1.0.0",
  "description": "Ifeoma Jonah Portifolio",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.css",
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "prefix:css": "postcss css/style.css --use=autoprefixer --output=css/style-prefix.css",
    "compress:css": "node-sass css/style-prefix.css css/style.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonasAnn/IfeomaJonah.git"
  },
  "keywords": [
    "portfolio",
    "sass",
    "npm"
  ],
  "author": "Ifeoma Jonah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JonasAnn/IfeomaJonah/issues"
  },
  "homepage": "https://github.com/JonasAnn/IfeomaJonah#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "postcss-cli": "^7.1.1"
  }
}
